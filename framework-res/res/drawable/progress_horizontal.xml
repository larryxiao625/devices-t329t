<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#fff3f0e9" android:endColor="#fff3f0e9" android:angle="270.0" android:centerY="0.75" android:centerColor="#fff3f0e9" />
        </shape>
    </item>
    <item android:id="@id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#801596c6" android:endColor="#a017a7dd" android:angle="270.0" android:centerY="0.75" android:centerColor="#8033b5e5" />
            </shape>
        </clip>
    </item>
    <item android:id="@id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff1596c6" android:endColor="#ff17a7dd" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff33b5e5" />
            </shape>
        </clip>
    </item>
</layer-list>